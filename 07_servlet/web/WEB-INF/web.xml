<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>TestHttpServletRequest</servlet-name>
        <servlet-class>servlet_test.TestHttpServletRequest</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ParameterServlet</servlet-name>
        <servlet-class>servlet_test.ParameterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servlet1</servlet-name>
        <servlet-class>servlet_test.Servlet1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servlet2</servlet-name>
        <servlet-class>servlet_test.Servlet2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ResponseIOServlet</servlet-name>
        <servlet-class>servlet_test.ResponseIOServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Response1Servlet</servlet-name>
        <servlet-class>servlet_test.Response1Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Response2Servlet</servlet-name>
        <servlet-class>servlet_test.Response2Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Response2Servlet</servlet-name>
        <url-pattern>/response2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Response1Servlet</servlet-name>
        <url-pattern>/response1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ResponseIOServlet</servlet-name>
        <url-pattern>/ioServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TestHttpServletRequest</servlet-name>
        <url-pattern>/request</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>ParameterServlet</servlet-name>
        <url-pattern>/param</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>Servlet1</servlet-name>
        <url-pattern>/servlet1</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>Servlet2</servlet-name>
        <url-pattern>/servlet2</url-pattern>
    </servlet-mapping>
</web-app>