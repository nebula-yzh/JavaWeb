        <author>霍金</author>  <!--author 标签表示作者-->
        <price>75</price>      <!--price 标签表示价格-->
    </book>
    <book sn="SN5631251">  <!--book表示一个图书信息  sn属性表示图书序列号-->
        <name>java从入门到精通
            <![CDATA[
                这其中内容是纯文本
            ]]></name>  <!--name 标签表示书名-->
        <author>jjj</author>  <!--author 标签表示作者-->
        <price>8.8</price>      <!--price 标签表示价格-->
    </book>
</books>